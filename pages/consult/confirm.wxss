page{
    background-color: #3693c9;
  }
  .conta{
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-left: 25rpx;
    padding-right: 25rpx;
  }
  
  .title{
    letter-spacing:15rpx;
    margin-top: 15px;
    font-weight:bold;
    font-size: 35rpx;
  }
  
  .cent{
    margin-top: 58rpx;
  }
  .log-txt{
    background-color: #ffffff;
    border:2px solid #fff;
    padding-bottom: 20px;
    height: 70vh;
  }
  
  text{
    font-size: 15px;
    color: #0a0b0e;
  }
  
  .button{
    margin-top: 28rpx;
    background-color: #44D7BA;
    box-shadow: 0px 0px 30px #ccc;
    width: 100%;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    color: #fff;
  }