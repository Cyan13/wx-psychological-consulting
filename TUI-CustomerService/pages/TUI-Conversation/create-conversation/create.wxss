/*分割线样式*/
.div-line{
  background: #E0E3DA;
  width: 100%;
  height: 3rpx;
}

.item {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 120rpx;
}
.marginTop {
  margin-top: 30rpx;
}
.consultContainer{
  background-color: #ffffff;
}
.consult-list{
  /* position: absolute; */
  /* top: 176rpx; */
  /* margin-top: 176rpx; */
  width: 750rpx;
}
.item .poster {
  width: 120rpx;
  height: 120rpx;
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.item .title{
  color: #333333;
  font-size: 38rpx;
  flex-grow: 1;
}
.item .meta {
  flex: 1;
}
.item .meta .artists {
  font-size: 26rpx;
  color: #999;
}
.item .rating {
  font-size: 28rpx;
  font-weight: bold;
  color: #3a7a38;
}
